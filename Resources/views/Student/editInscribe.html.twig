{% extends "EsolvingEschoolUserBundle::template.html.twig" %}
{% trans_default_domain 'EsolvingEschoolUserBundle'%}
{% block title %}{% trans %}edit_student_inscribed{% endtrans %}{% endblock %}
{% block content %}
<div id="content">
    <h1>{{ block('title')}}</h1>
    <div>{% trans %}current_date{% endtrans %} {{ date() | localizeddate('long') }}</div>
    {% if app.session.hasFlash('notice') %}
    <div class="flash-notice">
        {{ html.jqueryUiGetNotice(app.session.flash('notice'),'1') }}
    </div>
{% endif %}
    <form action="" method="post" {{ form_enctype(form) }} novalidate="">
        {{ form_widget(form) }}
            <div class="form_label">&nbsp;</div>
            <div class="form_widget"><input type="submit" value="{% trans %}edit{% endtrans %}" class="button button-form"></div>
        </form>
        <ul>
            <li><a href="{{ path('esolving_eschool_roomB_Student_list_inscribe') }}">{% trans %}back_to_the_list{% endtrans %}</a></li>
            <li><a href="{{ path('esolving_eschool_userB_Student_delete_inscribe',{'studentInscribeId':studentInscribeId}) }}">{% trans %}delete{% endtrans %}</a></li>
        </ul>
    </div>
{% endblock content%}

